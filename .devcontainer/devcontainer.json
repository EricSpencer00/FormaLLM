{
  "name": "TLA+ LLM Research",
  "build": {
    "context": "..",
    "dockerfile": "Dockerfile"
  },
  "containerEnv": {
    "DOTENV_CONFIG_PATH": "/workspaces/FormaLLM/.env"
  },
  "runArgs": [
    "--env-file", ".env"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "remoteUser": "root"
}